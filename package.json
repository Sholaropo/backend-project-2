{
  "name": "backend-project-2",
  "version": "1.0.0",
  "main": "dist/app.js",
  "module": "dist/app.js",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/app.js",
    "dev": "cross-env NODE_ENV=development tsx src/app.ts",
    "dev:watch": "cross-env NODE_ENV=development tsx watch src/app.ts",
    "build": "tsc",
    "test": "cross-env NODE_ENV=test node --no-warnings=ExperimentalWarning --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "cross-env NODE_ENV=test node --no-warnings=ExperimentalWarning --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "cross-env NODE_ENV=test node --no-warnings=ExperimentalWarning --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:ci": "cross-env NODE_ENV=test node --no-warnings=ExperimentalWarning --experimental-vm-modules node_modules/jest/bin/jest.js --ci --coverage --runInBand",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cross-env": "^7.0.3",
    "express": "^4.21.2",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
